<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.doctor_fish_back.repository.admin.AdminRoleMapper">


    <select id="getRoles" resultType="com.project.doctor_fish_back.entity.Role">
        select
            id,
            name,
            position
        from
            role_tb
        where
            id != 1
    </select>
</mapper>